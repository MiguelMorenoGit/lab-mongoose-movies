<a href="/">Go back to Homepage</a>
<h2>Index of Celebrities</h2>
<ul>
  {{#each allCelebrities}}
    <li><a href="/celebrities/{{this._id}}">{{this.name}}</a>
      <form action="/celebrities/{{this._id}}/delete" method="POST">
        <button type="submit">Delete</button>
      </form>
    </li>
  {{/each}}
</ul>
<button>
  <a class = "button" href="/celebrities/new">create a new Celebrity</a>
</button>